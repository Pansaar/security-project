{
    "compilerOptions": {
      "target": "es2017",
      "lib": [
        "ES2015",
        "dom"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "noEmit": true,
      "incremental": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve"
    },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx"
  , "pages/searchProduct", "pages/slipVerify", "pages/slipVerify"],
    "exclude": [
      "node_modules"
    ]
  }